{
	"name": "animeyoutube-downloader",
	"version": "1.0.0",
	"description": "Download the anime you want from AnimeYoutube without any of the ads. This project is not rpduction ready because of some issues. Please check the issues section if you can help me out with any of them.",
	"scripts": {
		"build": "npm run build --prefix client",
		"client": "npm start --prefix client",
		"dev": "concurrently \"npm start\" \"npm run client\"",
		"install": "npm install --prefix client && pipenv install",
		"start": "python wsgi.py"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jagannathBhat/animeyoutube-downloader.git"
	},
	"keywords": [
		"anime",
		"downloader",
		"flask",
		"react",
		"reactjs",
		"scraper"
	],
	"author": "Jagannath Bhat",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/jagannathBhat/animeyoutube-downloader/issues"
	},
	"homepage": "https://github.com/jagannathBhat/animeyoutube-downloader#readme",
	"dependencies": {
		"concurrently": "^5.1.0"
	}
}
